<!-- 마이페이지 > 쪽지함 -->
<template name="customerWrite">
  <div class="h-scroll hi-scroll">
    <!-- mypage-title -->
    <div class="mypage-title">
      <h2>1:1 문의하기</h2>
    </div>
    <!-- mypage-title -->
    <!-- content -->
    <div class="content">
      <div class="tab-content inner line">
        <!-- tab-pane -->
        <div id="profile" class="tab-pane fade in active">
          <div class="mypage-form" style="width:100%;">
            <!-- row -->
            <div class="row">
              <div class="col-xs-2">제목</div>
              <div class="col-xs-10">
                <input type="text" placeholder="" id="title" style="width:100%;"/>
              </div>
            </div>
            <!-- row -->
            <!-- row -->
            <div class="row">
              <div class="col-xs-2">내용</div>
              <div class="col-xs-10">
                <textarea class="" id="question" rows="0" cols="0" style="width:100%; height:300px;">
                </textarea>
              </div>
            </div>
            <!-- row -->
            <!-- row -->
            <div class="row">
              <div class="col-xs-2">파일첨부</div>
              <div class="col-xs-10" id="fileList">
                <div id="fileSet" name="fileSet">
                <input type="file" placeholder="" id="inputFile1" name="inputFile1" style="width:210px;"/><a class="btn-tagDelete" id='frListTag'></a>
                </div>
                <!-- <div class="input-article icon-style" id="fileList">
                    <div id="fileSet" style="width:440px;">
                      <input type="file" placeholder="" id="inputFile1" style="width:210px;"/><a class="btn-tagDelete" id='frListTag'></a>
                      <a href="javascript:document.getElementById('inputFile').click();" style="width:40px;">파일찾기</a>
                    </div>
                </div> -->
              </div>
              <a href="javascript:void(0)" class="mypage-btn inner" id="addAtachFile">파일추가</a>
            </div>
            <!-- row -->
          </div>
          <!-- mypage-content-btnSet -->
          <div class="mypage-content-btnSet text-center mt30">
            <a href="javascript:void(0)" class="mypage-cancle" id="cancel">취소</a>
            <a href="javascript:void(0)" class="mypage-add" id="save">저장</a>
          </div>
          <!-- //mypage-content-btnSet -->
        </div>
        <!-- //tab-pane -->
      </div>
    </div>
  </div>
</template>
